{
  "name": "chemical-hazard-tool",
  "version": "0.1.0",
  "description": "GUI for the chemical hazard service.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public ./.cache ./dist",
    "cleanup": "npm run clean",
    "dev": "set SERVER=http://localhost:3333&& parcel index.html",
    "start": "parcel index.html",
    "build:domain": "npm run clean && parcel build index.html --out-dir ./public",
    "keycloak": "docker-compose up -d -f ./docker/docker-compose.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/chemical-hazard-tool.git"
  },
  "keywords": [
    "SPA",
    "mithril",
    "location register",
    "REST",
    "database",
    "lokijs",
    "dynamic",
    "forms"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/chemical-hazard-tool/issues"
  },
  "homepage": "https://github.com/erikvullings/chemical-hazard-tool#readme",
  "dependencies": {
    "leaflet": "^1.6.0",
    "leaflet-draw": "^1.0.4",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril": "^2.0.4",
    "mithril-leaflet": "^0.2.2",
    "mithril-materialized": "^0.16.1",
    "mithril-ui-form": "^0.6.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.5.9",
    "@types/lokijs": "^1.5.3",
    "@types/materialize-css": "^1.0.7",
    "@types/mithril": "^2.0.2",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
