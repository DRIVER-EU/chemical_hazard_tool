{
  "name": "chemical-hazard-tool-adapter",
  "version": "0.1.0",
  "description": "Service that sends chemical hazard requests to Kafka, and pushes chemical hazard GeoJSON back.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start:old": "tsc-watch --onSuccess \"node ./dist/index.js\" --onFailure \"echo Compilation Failed\"",
    "start": "ts-node-dev src/index.ts",
    "dev": "npm run start",
    "build": "tsc",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DRIVER-EU/chemical-hazard-tool.git"
  },
  "keywords": [
    "kafka",
    "kafka-node",
    "driver+",
    "test-bed",
    "crisis",
    "management"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DRIVER-EU/chemical-hazard-tool/issues"
  },
  "homepage": "https://github.com/DRIVER-EU/chemical-hazard-tool#readme",
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/swagger": "^4.1.14",
    "@nestjs/websockets": "^6.10.13",
    "@nestjs/platform-express": "^6.10.13",
    "@nestjs/platform-socket.io": "^6.10.13",
    "swagger-ui-express": "^4.1.2",
    "express": "^4.17.1",
    "body-parser": "^1.19.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "node-test-bed-adapter": "^0.8.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/geojson": "^7946.0.7",
    "@types/body-parser": "^1.17.0",
    "@types/express-serve-static-core": "^4.17.1",
    "@types/node": "^13.1.2",
    "@types/socket.io": "^2.1.4",
    "kafka-node": "^5.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "3.7.5"
  }
}
